#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include<math.h>
#include<string.h>
#include<windows.h>
int main()
{
	int a=0;
	int b=0;
	int c=0;
	printf("s输入三个数->");
	scanf("%d %d %d",&a,&b,&c);
	if(a<b)
	{
		int tmp = a;  //tmp是临时变量
		a = b;
		b = tmp;
	}
	if(a<c)
	{
      int tmp = a;
	  a = c;
	  c = tmp;
	}
	if(b<c)
	{
		int tmp = b;
		b = c;
		c = tmp;
	}
	printf("%d %d %d\n",a,b,c);
		return 0;
}

{
	int c = 0;
	static int b = 3;
	c += 1;
	b += 2;
	return (a+b+c);
}
int main()
{
	int i;
	int a = 2;
	for(i = 0;i<5;i++)
	{
		printf("%d\n",sum(a));
	}
return 0;
}
